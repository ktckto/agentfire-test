{% if user_id>0 %}
<label>
    Show only my markers
    <input id="filter-own" type="checkbox">
</label>
{% endif %}
<ul id="tags-list">
    {% for tag in tags %}
        <li>
            <label>
                {{ tag.name }}
                <input class="filter-checkbox" type="checkbox" name="{{ tag.id }}" checked>
            </label>

        </li>
    {% endfor %}
</ul>
